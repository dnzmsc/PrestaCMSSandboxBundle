{
    "name": "presta/cms-sandbox-bundle",
    "type": "symfony-bundle",
    "description": "PrestaCMS Sandbox bundle : PrestaCMS Demonstration bundle",
    "keywords": ["symfony2", "bundle", "cms", "symfony-cmf", "prestaconcept"],
    "homepage": "https://github.com/prestaconcept/PrestaCMSSandboxBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bastien",
            "email": "nbastien@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "presta/presta-cms": "1.0.*"
    },
    "require-dev": {
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/doctrine-fixtures-bundle": "2.1.*",
        "jackalope/jackalope-doctrine-dbal": "1.0.*@dev",
        "symfony-cmf/testing": "1.0.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Presta\\CMSSandboxBundle": "" }
    },
    "target-dir": "Presta/CMSSandboxBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

